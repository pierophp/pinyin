  "description": "A Vue.js project",
  "author": "Piero Giusti <pierophp@gmail.com>",
  "private": true,
  "scripts": {
    "dev-base": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dev": "export BUILD_ENTRYPOINT=main.editor; yarn dev-base",
    "build": "export BUILD_ENTRYPOINT=main.editor; node build/build.js",
    "dev-bible": "export BUILD_ENTRYPOINT=main.bible; yarn dev-base",
    "build-bible": "export BUILD_ENTRYPOINT=main.bible; node build/build.js",
    "dev-dictionary": "export BUILD_ENTRYPOINT=main.dictionary; yarn dev-base",
    "build-dictionary": "export BUILD_ENTRYPOINT=main.dictionary; node build/build.js",
    "dev-videos": "export BUILD_ENTRYPOINT=main.videos; yarn dev-base",
    "build-videos": "export BUILD_ENTRYPOINT=main.videos; node build/build.js",
    "unit": "jest test/unit/specs --coverage",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs"
  },
  "dependencies": {
